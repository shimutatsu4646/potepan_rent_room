<%= @user.email %>
<%= @user.password %>
<%= link_to "変更する", edit_user_path(@user) %>

<%= form_with model: @user, html: { method: :patch } do |f| %>
    <%= f.label :icon_image %>
    <%= f.file_field :icon_image %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :introduction %>
    <%= f.text_area :introduction, size: "30x4" %>
    <%= f.submit "更新する" %>
<% end %>
